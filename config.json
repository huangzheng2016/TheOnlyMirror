{
  "port":8080,
  "tls":true,
  "tls_redirect":true,
  "crt":"",
  "key":"",
  "sources":{
    "ubuntu":{
      "path":"/ubuntu",
      "mirrors":[
        "https://archive.ubuntu.com"
      ]
    },
    "debian":{
      "path":"/debian",
      "mirrors":[
        "https://deb.debian.org"
      ]
    },
    "centos":{
      "path":"/centos",
      "mirrors":[
        "http://mirror.centos.org"
      ]
    },
    "docker_auth":{
      "ua":"docker",
      "path":"/token",
      "mirrors":[
        "https://auth.docker.io"
      ]
    },
    "docker_registry":{
      "ua":"docker",
      "replaces":[
        {
          "type":"header",
          "header": "www-authenticate",
          "src":"https://",
          "dst":"<TLS_SCHEME>"
        },
        {
          "type":"header",
          "header": "www-authenticate",
          "src":"auth.docker.io",
          "dst":"<HOST>"
        }
      ],
      "mirrors":[
        "https://registry-1.docker.io"
      ]
    },
    "pypi_package":{
      "ua":"pip",
      "path":"/packages",
      "mirrors":[
        "https://files.pythonhosted.org"
      ]
    },
    "pypi_web":{
      "ua":"pip",
      "replaces":[
        {
          "src":"https://files.pythonhosted.org",
          "dst":""
        }
      ],
      "prefix":"/simple",
      "mirrors":[
        "https://pypi.org"
      ]
    }
  }
}