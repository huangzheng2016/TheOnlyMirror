{
  "port":8080,
  "tls":false,
  "tls_redirect":false,
  "crt":"",
  "key":"",
  "sources":{
    "ubuntu":{
      "path":"/ubuntu",
      "mirror":"http://archive.ubuntu.com"
    },
    "debian":{
      "path":"/debian",
      "mirror":"https://deb.debian.org"
    },
    "centos":{
      "path":"/centos",
      "mirror":"http://mirror.centos.org"
    },
    "kali":{
      "path":"/kali",
      "mirror":"https://http.kali.org"
    },
    "alpine":{
      "path":"/alpine",
      "mirror":"https://dl-cdn.alpinelinux.org"
    },
    "github":{
      "ua":"git",
      "mirror":"https://github.com"
    },
    "docker_auth":{
      "priority":2,
      "ua":"docker",
      "path":"/token",
      "mirror":"https://auth.docker.io"
    },
    "docker_registry":{
      "priority":1,
      "ua":"docker",
      "replaces":[
        {
          "type":"header",
          "header": "www-authenticate",
          "src":"https://",
          "dst":"<TLS_SCHEME>"
        },
        {
          "type":"header",
          "header": "www-authenticate",
          "src":"auth.docker.io",
          "dst":"<HOST>"
        }
      ],
      "mirror":"https://registry-1.docker.io"
    },
    "pypi_package":{
      "priority":2,
      "ua":"pip",
      "path":"/packages",
      "mirror":"https://files.pythonhosted.org"
    },
    "pypi_web":{
      "priority":1,
      "ua":"pip",
      "replaces":[
        {
          "src":"https://files.pythonhosted.org",
          "dst":""
        }
      ],
      "prefix":"/simple",
      "mirror": "https://pypi.org"
    },
    "npm":{
      "ua":"npm",
      "replaces":[
        {
          "src":"https://",
          "dst":"<TLS_SCHEME>"
        },
        {
          "src":"registry.npmjs.org",
          "dst":"<HOST>"
        }
      ],
      "mirror":"https://registry.npmjs.org"
    }
  },
  "proxy":[
    "https://github.com",
    "https://raw.githubusercontent.com",
    "https://gist.githubusercontent.com",
    "https://gist.github.com"
  ]
}